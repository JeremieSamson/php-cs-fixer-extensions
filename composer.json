{
    "name": "slam/php-cs-fixer-extensions",
    "description": "Slam extension of friendsofphp/php-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "slam/php-debug-r": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SlamCsFixer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamCsFixer\\Tests\\": "tests/"
        }
    }
}
